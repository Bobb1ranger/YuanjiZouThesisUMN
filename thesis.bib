@article{MixH_2infcontrolforrandomsys,
title = {Robust mixed H2/H$\infty$ control of networked control systems with random time delays in both forward and backward communication links},
journal = {Automatica},
volume = {47},
number = {4},
pages = {754-760},
year = {2011},
issn = {0005-1098},
doi = {https://doi.org/10.1016/j.automatica.2011.01.022},
url = {https://www.sciencedirect.com/science/article/pii/S0005109811000379},
author = {Yang Shi and Bo Yu},
keywords = {Robust control, Networked control systems, Mixed  control, Time delays, Packet dropouts, Markov chains},
abstract = {This paper is concerned with the two-mode-dependent robust control synthesis of networked control systems where random delays existing in both forward controller-to-actuator (C–A) and feedback sensor-to-controller (S–C) communication links are modeled as Markov chains. The output feedback controller is designed to depend on the current S–C delay and the previous C–A delay. Then, the closed-loop system is formulated as a special jump linear system. The generalized definitions of the H2 and H∞ norms for such underlying special systems are proposed. Further, the two-mode-dependent robust H2 and robust mixed H2/H∞ control design methods for NCSs are developed. The design examples illustrate the effectiveness of the proposed methods.}
}


